{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
		"path": "pages/home/home",
		"style": {
			"navigationBarTitleText": "首页",
			"enablePullDownRefresh": true,
			"navigationStyle": "custom" // 隐藏系统导航栏
			// "navigationBarTextStyle": "white" // 状态栏字体为白色，只能为 white-白色，black-黑色 二选一
		}
	}, {
		"path": "pages/list/list",
		"style": {
			"navigationBarTitleText": "产品",
			"enablePullDownRefresh": true,
			"navigationStyle": "custom" // 隐藏系统导航栏
		}
	}, {
		"path": "pages/me/me",
		"style": {
			"navigationBarTitleText": "我的",
			"navigationStyle": "custom" // 隐藏系统导航栏
		}
	}, {
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "首页"
		}
	}],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "嘉E保",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#FFFFFF"
	},
	"tabBar": {
		"color": "#C6C9CC",
		"selectedColor": "#1246FF",
		"backgroundColor": "#FFFFFF",
		// #ifdef APP-PLUS
		"blurEffect": "light",
		// #endif
		"borderStyle": "black",
		"list": [{
				"pagePath": "pages/home/home",
				"iconPath": "static/img/home.png",
				"selectedIconPath": "static/img/selected-home.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/list/list",
				"iconPath": "static/img/list.png",
				"selectedIconPath": "static/img/selected-list.png",
				"text": "产品"
			},
			{
				"pagePath": "pages/me/me",
				"iconPath": "static/img/me.png",
				"selectedIconPath": "static/img/selected-me.png",
				"text": "我的"
			}
		]
	}
}
